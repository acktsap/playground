{
  "manifest_version": 2,
  "name": "browser-extension-test",
  "description": "test",
  "version": "0.0.1",
  "icons": {
    "32": "icons/beasts-32.png",
    "48": "icons/beasts-48.png"
  },
  "background": {
    "scripts": [
      "background_scripts/index.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "content_scripts/index.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/beast-32.png",
    "default_title": "browser-extension",
    "default_popup": "popup/index.html"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "web_accessible_resources": [
    "img/frog.jpg",
    "img/snake.jpg",
    "img/turtle.jpg"
  ]
}